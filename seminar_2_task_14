class Song:
    def __init__(self, title, artist, release_year):
        self.t = title
        self.a = artist
        self.r = release_year
    def __str__(self):
        string_info = str(self.__dict__)
        return string_info
    
some_song = Song('Какая-то песня', 'Кто-то', 2024)

print(some_song)